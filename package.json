{
  "name": "@kodadot1/click",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "build": "rm -rf lib && tsc",
    "update": "npx npm-check-updates --filter /subsquid/ --upgrade && npm i -f",
    "db:migrate": "npx squid-typeorm-migration apply",
    "processor:start": "node lib/processor.js",
    "query-node:start": "squid-graphql-server",
    "lint": "npm run lint:prod -- --fix",
    "lint:prod": "eslint . --ext .ts --ignore-path .gitignore --max-warnings 0"
  },
  "dependencies": {
    "@kodadot1/metasquid": "^0.2.1-rc.1",
    "@kodadot1/minipfs": "^0.4.0-rc.0",
    "@subsquid/archive-registry": "^3.1.0",
    "@subsquid/frontier": "^0.2.0",
    "@subsquid/graphql-server": "^3.3.2",
    "@subsquid/ss58": "^0.1.4",
    "@subsquid/substrate-processor": "^2.4.1",
    "@subsquid/typeorm-migration": "^0.1.6",
    "@subsquid/typeorm-store": "^0.2.2",
    "axios": "^1.2.1",
    "dotenv": "^16.0.3",
    "ethers": "^6.6.5",
    "md5": "^2.3.0",
    "nanoid": "3.3.4",
    "pg": "^8.9.0",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@subsquid/evm-typegen": "^3.2.2",
